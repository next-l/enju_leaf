<%= form_for(@bookmark_stat) do |f| -%>
  <%= error_messages(@bookmark_stat) -%>

  <div class="field">
    <%= f.label :start_date -%><br />
    <%= f.datetime_select :start_date -%>
  </div>
  <div class="field">
    <%= f.label :end_date -%><br />
    <%= f.datetime_select :end_date -%>
  </div>
  <div class="field">
    <%= f.label :note -%><br />
    <%= f.text_area :note -%>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<%- end -%>
