<div id="content_detail" class="ui-corner-all">
<h1 class="title"><%= t('page.showing', :model => t('activerecord.models.extent')) -%></h1>
<div id="content_list">
<p id="notice"><%= notice %></p>

<p>
  <strong><%= t('activerecord.attributes.extent.name') -%>:</strong>
  <%= @extent.name -%>
</p>

<p>
  <strong><%= t('activerecord.attributes.extent.display_name') -%>:</strong>
  <%= @extent.display_name.localize -%>
</p>

<p>
  <strong><%= t('activerecord.attributes.extent.note') -%>:</strong>
  <%= @extent.note -%>
</p>

</div>
</div>

<div id="submenu" class="ui-corner-all">
  <ul>
    <li><%= link_to t('page.edit'), edit_extent_path(@extent) -%></li>
    <li><%= link_to t('page.back'), extents_path -%></li>
  </ul>
</div>
