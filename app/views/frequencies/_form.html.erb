<%= form_for(@frequency) do |f| %>
  <%= f.error_messages %>

  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :display_name %><br />
    <%= f.text_area :display_name %>
  </div>
  <div class="field">
    <%= f.label :note %><br />
    <%= f.text_area :note %>
  </div>
  <div class="field">
    <%= f.label :freq_string %><br />
    <%= f.text_field :freq_string %>
  </div>
  <div class="field">
    <%= f.label :nii_code %><br />
    <%= f.text_field :nii_code %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
