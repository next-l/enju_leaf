<div id="content_detail">
<h1 class="title"><%= t('page.showing', :model => t('activerecord.models.patron_import_file')) -%></h1>
<div id="content_list">
<p>
  <strong>Id:</strong>
  <%= @patron_import_file.id -%>
</p>

<p>
  <strong><%= t('activerecord.attributes.patron_import_file.patron_import_file_name') -%>:</strong>
  <%= link_to h(@patron_import_file.patron_import_file_name), patron_import_file_path(@patron_import_file, :format => :download) -%>
</p>

<p>
  <strong><%= t('activerecord.attributes.patron_import_file.patron_import_content_type') -%>:</strong>
  <%= @patron_import_file.patron_import_content_type -%>
</p>

<p>
  <strong><%= t('activerecord.attributes.patron_import_file.patron_import_file_size') -%>:</strong>
  <%= @patron_import_file.patron_import_file_size -%>
</p>

<p>
  <strong><%= t('activerecord.attributes.patron_import_file.file_hash') -%>:</strong>
  <%= @patron_import_file.file_hash -%>
</p>

<p>
  <strong><%= t('page.created_at') -%>:</strong>
  <%= l(@patron_import_file.created_at) if @patron_import_file.created_at -%>
</p>

<p>
  <strong><%= t('activerecord.attributes.patron_import_file.imported_at') -%>:</strong>
  <%= l(@patron_import_file.imported_at) if @patron_import_file.imported_at -%>
</p>

<p>
  <strong><%= t('activerecord.attributes.manifestation_import_file.state') -%>:</strong>
  <%= localized_state(@patron_import_file.state) -%>
</p>

<p>
  <strong><%= t('activerecord.models.user') -%>:</strong>
  <%= link_to h(@patron_import_file.user.username), user_path(@patron_import_file.user.username) -%>
</p>

<table class="index">
  <%- @patron_import_file.imported_objects.each do |object| -%>
    <tr class="line<%= cycle("0", "1") -%>">
      <td><%= link_to h(object.importable.full_name), object.importable -%></td>
      <td><%= link_to h(object.importable.user.username), user_path(object.importable.login) if object.importable.user -%></td>
      <td><%= l(object.created_at) -%></td>
    </tr>
  <%- end -%>
</table>

</div>
</div>

<div id="submenu">
  <ul>
    <li><%= link_to t('page.edit'), edit_patron_import_file_path(@patron_import_file) -%></li>
    <li><%= link_to t('page.back'), patron_import_files_path -%></li>
  </ul>
</div>
