<div id="content_detail" class="col col-md-9">
<h1 class="title card-header"><%= t('page.editing', model: t('activerecord.models.resource_import_file')) -%></h1>
<div id="content_list" class="card-body">

<%= form_for(@resource_import_file) do |f| -%>
  <%= error_messages(@resource_import_file) -%>

  <div class="field">
    <%= f.label :note -%><br />
    <%= f.text_area :note -%>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<%- end -%>

</div>
</div>

<div id="submenu" class="col">
  <ul>
    <li><%= link_to t('page.show'), @resource_import_file -%></li>
    <li><%= link_to t('page.back'), resource_import_files_path -%></li>
  </ul>
</div>
