<div id="content_detail" class="ui-corner-all">
<h1 class="title"><%= t('page.listing', :model => t('activerecord.models.event_import_result')) -%></h1>
<div id="content_list">
  <% if @event_import_file %>
    <h2 class="event_title">
      <%= link_to @event_import_file.event_import_file_name, @event_import_file %>
    </h2>
  <% end %>
<%= t('activerecord.attributes.event_import_result.imported_row') -%>: <%= @results_num if @results_num -%>
<table class="index">
  <tr>
    <th><%= t('activerecord.models.event') %></th>
    <th><%= t('activerecord.attributes.event_import_result.error_msg') %></th>
    <th></th>
  </tr>

<% @event_import_results.each do |event_import_result| %>
  <tr class="line<%= cycle("0", "1") -%>">
    <td><%= link_to event_import_result.event.display_name.localize, event_import_result.event if event_import_result.event %></td>
    <td><%= event_import_result.error_msg %></td>
    <td>
      <%= link_to t('page.show'), event_import_result %>
      <%= link_to t('page.destroy'), event_import_result, :confirm => t('page.are_you_sure'), :method => :delete %>
    </td>
  </tr>
<% end %>
</table>

<%= will_paginate(@event_import_results) %>

</div>
</div>

<div id="submenu" class="ui-corner-all">
  <ul>
    <li><%= link_to t('page.back'), @event_import_file %></li>
  </ul>
  <% if @event_import_file %>
    <%= link_to (image_tag 'icons/page_white_excel.png', :Size => '16x16', :alt => 'TSV', :title => 'TSV', :class => 'icon'), event_import_file_event_import_results_path(@event_import_file, :locale => @locale.to_s, :format => 'tsv') -%>
    (<%= link_to 'TSV', event_import_file_event_import_results_path(:locale => @locale.to_s, :format => 'tsv') -%>)
  <% else %>
    <%= link_to (image_tag 'icons/page_white_excel.png', :size => '16x16', :alt => 'TSV', :title => 'TSV', :class => 'icon'), event_import_results_path(:locale => @locale.to_s, :format => "tsv") -%>
    (<%= link_to 'TSV', event_import_results_path(:locale => @locale.to_s, :format => "tsv") -%>)
  <% end %>
</div>
