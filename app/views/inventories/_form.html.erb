<%= form_for(@inventory) do |f| -%>
  <%= error_messages(@inventory) -%>

  <div class="field">
    <%= f.label :item_id -%><br />
    <%= f.text_field :item_id -%>
  </div>
  <div class="field">
    <%= f.label :inventory_file_id -%><br />
    <%= f.text_field :inventory_file_id -%>
  </div>
  <div class="field">
    <%= f.label :note -%><br />
    <%= f.text_area :note -%>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<%- end -%>
