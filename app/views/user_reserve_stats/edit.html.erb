<div id="content_detail">
<h1 class="title"><%= t('page.editing', :model => t('activerecord.models.user_reserve_stat')) -%></h1>
<div id="content_list">

<%= form_for(@user_reserve_stat) do |f| -%>
  <%= f.error_messages -%>

  <p>
    <%= f.label t('activerecord.attributes.user_reserve_stat.start_date') -%><br />
    <%= f.datetime_select :start_date -%>
  </p>
  <p>
  <%= f.label t('activerecord.attributes.user_reserve_stat.end_date') -%><br />
    <%= f.datetime_select :end_date -%>
  </p>
  <p>
    <%= f.label t('activerecord.attributes.user_reserve_stat.note') -%><br />
    <%= f.text_area :note -%>
  </p>
  <p>
    <%= f.submit t('page.update') -%>
  </p>
<%- end -%>

</div>
</div>

<div id="submenu">
  <ul>
    <li><%= link_to t('page.show'), @user_reserve_stat -%></li>
    <li><%= link_to t('page.back'), user_reserve_stats_path -%></li>
  </ul>
</div>
