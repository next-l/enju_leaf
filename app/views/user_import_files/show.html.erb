<div id="content_detail" class="ui-corner-all ui-widget-content">
<h1 class="title"><%= t('page.showing', model: t('activerecord.models.user_import_file')) -%></h1>
<div id="content_list">
<p id="notice"><%= notice %></p>

<p>
  <strong>Id:</strong>
  <%= @user_import_file.id -%>
</p>

<% if @user_import_file.attachment.attached? %>
<p>
  <strong><%= t('activerecord.attributes.user_import_file.user_import_file_name') -%>:</strong>
  <%= link_to @user_import_file.attachment.filename, rails_blob_path(@user_import_file.attachment) if @user_import_file.attachment.attached? %>
  (<%= @user_import_file.attachment.content_type -%>)
</p>

<p>
  <strong><%= t('activerecord.attributes.user_import_file.user_import_file_size') -%>:</strong>
  <%= @user_import_file.attachment.byte_size -%>
</p>
<% end %>

<p>
  <strong><%= t('page.created_at') -%>:</strong>
  <%= l(@user_import_file.created_at) if @user_import_file.created_at -%>
</p>

<p>
  <strong><%= t('activerecord.attributes.user_import_file.executed_at') -%>:</strong>
  <%= l(@user_import_file.executed_at) if @user_import_file.executed_at -%>
</p>

<p>
  <strong><%= t('activerecord.attributes.user_import_file.edit_mode') -%>:</strong>
  <%= @user_import_file.edit_mode -%>
</p>

<p>
  <strong><%= t('activerecord.attributes.user_import_file.user_encoding') -%>:</strong>
  <%= @user_import_file.user_encoding -%>
</p>

<p>
  <strong><%= t('activerecord.attributes.user_import_file.default_user_group_id') -%>:</strong>
  <%= link_to @user_import_file.default_user_group.display_name.localize, @user_import_file.default_user_group if @user_import_file.default_user_group.try(:display_name) -%>
</p>

<p>
  <strong><%= t('activerecord.attributes.user_import_file.default_library_id') -%>:</strong>
  <%= link_to @user_import_file.default_library.display_name.localize, @user_import_file.default_library if @user_import_file.default_library.try(:display_name) -%>
</p>

<p>
  <strong><%= t('activerecord.attributes.user_import_file.state') -%>:</strong>
  <%= localized_state(@user_import_file.current_state) -%>
</p>

<p>
  <strong><%= t('activerecord.attributes.user_import_file.error_message') -%>:</strong>
  <%= simple_format(@user_import_file.error_message) -%>
</p>

<p>
  <strong><%= t('activerecord.models.user') -%>:</strong>
  <%= link_to @user_import_file.user.username, @user_import_file.user.profile if @user_import_file.user.try(:profile) -%>
</p>

<% if @user_import_results %>
<%= render 'results' %>
<% end %>

</div>
</div>

<div id="submenu" class="ui-corner-all ui-widget-content">
  <%= form_for(@user_import_file) do |f| %>
  <ul>
    <li><%= back_to_index(flash[:page_info]) -%></li>
    <% if current_user.has_role?('Administrator') %>
      <li><%= link_to 'Resque', resque_path %></li>
    <% end %>
  </ul>
  <% end %>
</div>
